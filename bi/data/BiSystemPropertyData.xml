<entity-engine-xml>
    <SystemProperty systemResourceId="bi" systemPropertyId="dwh.initialised" systemPropertyValue="false"
        description="Whether the dwh has been initalised with past data. Choices are: 'true' or 'false'"/>
    <SystemProperty systemResourceId="bi" systemPropertyId="dwh.fromDate" systemPropertyValue="2000-01-01"
        description="The 'standard' start date for the initialisation of the DWH"/>
    <SystemProperty systemResourceId="bi" systemPropertyId="dwh.useTimeUomId" systemPropertyValue="TF_yr"
        description="The Time Frequecy UomId for future increments To determine future dates for the Date Dimension"/>
    <SystemProperty systemResourceId="bi" systemPropertyId="dwh.futureIncrement" systemPropertyValue="1"
        description="How many units for the Time Frequecy UomId. To determine future dates for the Date Dimension"/>
    <SystemProperty systemResourceId="bi" systemPropertyId="dwh.updateMode" systemPropertyValue="TYPE1"
        description="Update mode for DWH records. Per the DWT book."/>
</entity-engine-xml>